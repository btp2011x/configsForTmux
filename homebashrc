alias fromlabs='scp -r pete9934@kh4250-05.cselabs.umn.edu:~/csci2041/repo-pete9934/Lab_06/lab_06.ml ~/csci2041/repo-pete9934/Lab_06/lab_06.ml'
alias gcm='git commit -m'
alias ls='ls -G'
alias mux='tmuxinator'
alias pete='cd ~/csci2041/repo-pete9934'
alias rvm-restart='rvm_reload_flag=1 source '\''/Users/brianpeterson/.rvm/scripts/rvm'\'''
alias srcbash='source ~/.bash_profile'
alias tmux='mux default'
alias tolabs='scp ~/csci2041/repo-pete9934/Lab_06/lab_06.ml pete9934@kh4250-05.cselabs.umn.edu:~/csci2041/repo-pete9934/Lab_06/lab_06.ml'
alias vi='vim'
